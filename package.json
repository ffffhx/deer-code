{
  "name": "deer-code",
  "version": "0.1.0",
  "description": "A minimalist yet sufficient project for enabling everyone to learn how to develop an AI Coding Agent.",
  "main": "dist/main.js",
  "type": "module",
  "bin": {
    "deer-code": "./dist/main.js"
  },
  "scripts": {
    "dev": "tsx src/main.ts",
    "dev:silent": "npx tsx src/main.ts",
    "build": "tsc",
    "start": "node dist/main.js",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx"
  },
  "keywords": [
    "ai",
    "coding-agent",
    "langchain",
    "langgraph",
    "cli"
  ],
  "author": "Henry Li <henry1943@163.com>",
  "license": "MIT",
  "dependencies": {
    "@langchain/community": "^1.1.8",
    "@langchain/core": "^1.1.17",
    "@langchain/langgraph": "^1.1.2",
    "@langchain/openai": "^1.2.3",
    "chalk": "^5.4.1",
    "dotenv": "^16.4.7",
    "ink": "^5.1.0",
    "ink-text-input": "^6.0.0",
    "js-tiktoken": "^1.0.21",
    "minimatch": "^10.0.1",
    "node-pty": "^1.1.0",
    "react": "^18.3.1",
    "yaml": "^2.7.0",
    "yargs": "^18.0.0",
    "zod": "^3.24.1",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "@types/react": "^18.3.27",
    "@types/yargs": "^17.0.35",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "eslint": "^9.18.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
